<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_gps">

    <fragment
        android:id="@+id/navigation_gps"
        android:name="com.enib.toolty.ui.gps.GPSFragment"
        android:label="@string/title_gps"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_walk"
        android:name="com.enib.toolty.ui.Pedometer.PedometerFragment"
        android:label="@string/title_pedometer"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_scan"
        android:name="com.enib.toolty.ui.scan.ScanFragment"
        android:label="@string/title_scan"
        tools:layout="@layout/fragment_notifications" />
</navigation>